<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="NeptunProject.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:NeptunProject"
    Shell.FlyoutBehavior="Flyout"
    Title="NeptunProject">

    <ShellContent
    Title="Home"
    ContentTemplate="{DataTemplate local:MainPage}"
    Route="MainPage" />

    <ShellContent
    Title="Timetable"
    ContentTemplate="{DataTemplate local:View.TimetableView}"
    Route="Timetable" />

    <ShellContent
    Title="Subjects"
    ContentTemplate="{DataTemplate local:View.Subjects}"
    Route="Subjects" />

</Shell>
